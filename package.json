{
  "name": "filesfly",
  "version": "1.0.0",
  "description": "A blazingly fast S3 file uploader with a beautiful CLI interface",
  "author": "Ralf Boltshauser",
  "license": "MIT",
  "type": "module",
  "bin": {
    "ff": "./bin/ff.ts"
  },
  "scripts": {
    "start": "bun run ./bin/ff.ts",
    "build": "bun build ./bin/ff.ts --outdir dist",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\" \"tests/**/*.ts\" --fix",
    "test": "bun test"
  },
  "keywords": [
    "s3",
    "upload",
    "cli",
    "file-upload",
    "aws",
    "storage",
    "bun",
    "typescript",
    "filesfly"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ralf-boltshauser/filesfly.git"
  },
  "bugs": {
    "url": "https://github.com/ralf-boltshauser/filesfly/issues"
  },
  "homepage": "https://github.com/ralf-boltshauser/filesfly#readme",
  "engines": {
    "bun": ">=1.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@types/bun": "latest",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^9.0.0",
    "prettier": "^3.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@types/ansi-colors": "^3.2.6",
    "@types/cli-progress": "^3.11.6",
    "ansi-colors": "^4.1.3",
    "cli-progress": "^3.12.0",
    "commander": "^13.1.0"
  }
}
